{
  "name": "inscript",
  "version": "1.0.0",
  "description": "Promisified script loader",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "babel src -d lib",
    "dist": "browserify index.js -o dist/inscript.js",
    "minify": "uglifyjs --compress --mangle  -o ./dist/inscript.min.js -- ./dist/inscript.js",
    "start": "npm run build && npm run dist && npm run minify"
  },
  "keywords": [
    "script",
    "loader",
    "dependency",
    "management",
    "promises"
  ],
  "author": "Simon Tsvilik",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.7.5",
    "babel-preset-es2015": "^6.6.0",
    "browserify": "^13.0.0",
    "uglify-js": "^2.6.2"
  },
  "dependencies": {}
}
